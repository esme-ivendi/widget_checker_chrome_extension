{
    "manifest_version": 3,
    "name": "Widget Validator",
    "version": "1.0",
    "description": "Extracts widget iframe parameters",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  